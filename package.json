{
  "name": "hatton-website",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build --prefix-paths",
    "start:jaeger": "./run-jaeger.sh",
    "build:traced": "gatsby build --open-tracing-config-file node_modules/gatsby/dist/utils/tracer/jaeger-local.js",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "storybook": "cross-env NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "cross-env NODE_ENV=production build-storybook -s public",
    "test": "jest",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "forestry:preview": "gatsby develop -p 8080 -H 0.0.0.0",
    "forestry:publish": "gatsby build --prefix-paths",
    "publish-to-surge": "cd public && surge --domain hatton-solutions.surge.sh",
    "remove-from-surge": "surge teardown hatton-solutions.surge.sh"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.5.5",
